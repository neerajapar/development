pipeline {
  agent { label 'slavenodes'}
  stages {
    stage('create a dir') {
	  steps {
	    sh 'mkdir /tmp/testdir'
	  }
	}
	stage('find the java version') {
	  steps {
	    sh 'java -version'
	  }
	}
	stage('syatem uptime information') {
	  steps {
	    sh 'uptime'
	  }
	}
	stage('find out maven version') {
	  steps {
	    sh 'mvn -version'
	  }
	}
	stage('create 200 files') {
	  steps {
	    sh 'touch file.(1..200)'
	  }
	}
  }
}